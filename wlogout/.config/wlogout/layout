{
    "label" : "lock",
    "action" : "hyprlock",
    "keybind" : "l"
}
{
    "label" : "logout",
    "action" : "hyprctl dispatch exit",
    "keybind" : "e"
}
{
    "label" : "suspend",
    "action" : "pidof hyprlock || (hyprlock &); sleep 0.1; systemctl suspend",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "keybind" : "r"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "keybind" : "s"
}
